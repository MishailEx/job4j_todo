<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <servlet>
    <servlet-name>AddItem</servlet-name>
    <servlet-class>servlet.ItemServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddItem</servlet-name>
    <url-pattern>/add</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ToDo</servlet-name>
    <servlet-class>servlet.ToDoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ToDo</servlet-name>
    <url-pattern>/todo</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Filter</servlet-name>
    <servlet-class>servlet.FilterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Filter</servlet-name>
    <url-pattern>/fil</url-pattern>
  </servlet-mapping>
</web-app>
